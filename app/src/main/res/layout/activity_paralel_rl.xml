<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background"
    tools:context=".ParalelRL">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ParalelRC">

        <TextView
            android:id="@+id/textView20"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="@string/paralelrl"
            android:textAlignment="center"
            android:textColor="@color/renkli"
            android:textSize="20sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/buttonParalelRLHesapla"
            android:layout_width="90dp"
            android:layout_height="100dp"
            android:layout_marginEnd="20dp"
            android:backgroundTint="@color/mavisari"
            android:foreground="@drawable/hesapla"
            android:foregroundGravity="center"
            android:foregroundTint="@color/background"
            android:textAlignment="center"
            app:cornerRadius="100dp"
            app:layout_constraintBottom_toBottomOf="@+id/editTextParalelRLf"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/editTextParalelRLR" />

        <EditText
            android:id="@+id/editTextParalelRLR"
            android:layout_width="220dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:ems="10"
            android:hint="Direnç değerini giriniz (Ω)"
            android:inputType="number|numberDecimal"
            android:textAlignment="center"
            android:textColorHint="@color/renkli"
            android:textSize="14sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/buttonParalelRLHesapla"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/spinnerBobin" />

        <EditText
            android:id="@+id/editTextParalelRLV"
            android:layout_width="220dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:ems="10"
            android:hint="Gerilim değerini giriniz (V)"
            android:inputType="number|numberDecimal"
            android:textAlignment="center"
            android:textColorHint="@color/renkli"
            android:textSize="14sp"
            app:layout_constraintEnd_toStartOf="@+id/buttonParalelRLHesapla"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editTextParalelRLL" />

        <EditText
            android:id="@+id/editTextParalelRLf"
            android:layout_width="220dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:ems="10"
            android:hint="Frekans değerini giriniz (Hz)"
            android:inputType="number|numberDecimal"
            android:textAlignment="center"
            android:textColorHint="@color/renkli"
            android:textSize="14sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/buttonParalelRLHesapla"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editTextParalelRLV" />

        <TextView
            android:id="@+id/textViewParalelRLXL"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="50dp"
            android:textColor="@color/yazi"
            android:textSize="20sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editTextParalelRLf" />

        <TextView
            android:id="@+id/textViewParalelRLZ"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="90dp"
            android:textColor="@color/yazi"
            android:textSize="20sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editTextParalelRLf" />

        <TextView
            android:id="@+id/textViewParalelRLiR"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="170dp"
            android:textAlignment="textEnd"
            android:textColor="@color/yazi"
            android:textSize="20sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editTextParalelRLf" />

        <TextView
            android:id="@+id/textViewParelelRLiL"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="210dp"
            android:textAlignment="textEnd"
            android:textColor="@color/yazi"
            android:textSize="20sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editTextParalelRLf" />

        <TextView
            android:id="@+id/textViewParalelRLi"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="130dp"
            android:textAlignment="center"
            android:textColor="@color/yazi"
            android:textSize="20sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editTextParalelRLf" />

        <Spinner
            android:id="@+id/spinnerBobin"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:spinnerMode="dropdown"
            android:textAlignment="gravity"
            app:layout_constraintEnd_toStartOf="@+id/buttonParalelRLHesapla"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView20" />

        <EditText
            android:id="@+id/editTextParalelRLL"
            android:layout_width="220dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:ems="10"
            android:hint="Bobin değerini giriniz"
            android:inputType="number|numberDecimal"
            android:textAlignment="center"
            android:textColorHint="@color/renkli"
            android:textSize="14sp"
            app:layout_constraintEnd_toStartOf="@+id/buttonParalelRLHesapla"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editTextParalelRLR" />

        <com.google.android.gms.ads.AdView
            android:id="@+id/adView"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            app:adSize="BANNER"
            app:adUnitId="ca-app-pub-1879393212966846/1948543313"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            tools:ignore="MissingConstraints" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>