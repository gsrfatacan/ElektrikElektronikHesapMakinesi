<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".KirsofGerilimFragment">

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:text="@string/kirsofgerilim"
        android:textColor="@color/buton"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:src="@drawable/kirsofgerilimresim"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <EditText
        android:id="@+id/editTextKirsofR1"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_marginStart="90dp"
        android:layout_marginTop="35dp"
        android:layout_marginEnd="300dp"
        android:ems="10"
        android:gravity="center"
        android:hint="R1"
        android:inputType="number"
        android:textAlignment="center"
        android:textColor="@color/yazi"
        android:textColorHint="@color/yazi"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editTextKirsofR2"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_marginStart="180dp"
        android:layout_marginTop="35dp"
        android:layout_marginEnd="190dp"
        android:ems="10"
        android:gravity="center"
        android:hint="R2"
        android:inputType="number"
        android:textColor="@color/yazi"
        android:textColorHint="@color/yazi"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editTextKirsofR3"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_marginStart="240dp"
        android:layout_marginTop="35dp"
        android:layout_marginEnd="50dp"
        android:ems="10"
        android:gravity="center"
        android:hint="R3"
        android:inputType="number"
        android:textColor="@color/yazi"
        android:textColorHint="@color/yazi"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editTextKirsofGerilim"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_marginStart="70dp"
        android:layout_marginTop="110dp"
        android:layout_marginEnd="90dp"
        android:ems="10"
        android:hint="V"
        android:inputType="number"
        android:textAlignment="center"
        android:textColor="@color/yazi"
        android:textColorHint="@color/yazi"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextKirsofR2" />

    <TextView
        android:id="@+id/textViewKirsofGerilimVR1_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="90dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="300dp"
        android:textAlignment="center"
        android:textColor="@color/renkli"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextKirsofR1" />

    <TextView
        android:id="@+id/textViewKirsofGerilimVR2_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="180dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="190dp"
        android:textAlignment="center"
        android:textColor="@color/renkli"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextKirsofR2" />

    <TextView
        android:id="@+id/textViewKirsofGerilimVR3_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="240dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="50dp"
        android:textAlignment="center"
        android:textColor="@color/renkli"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextKirsofR3" />

    <TextView
        android:id="@+id/textViewKirsofGerilimI"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:text="Akım"
        android:textColor="@color/yazi"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/textViewKirsofGerilimIRes"
        app:layout_constraintTop_toBottomOf="@+id/buttonKirsofGerilimHesapla" />

    <TextView
        android:id="@+id/textViewKirsofGerilimVR1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="VR1"
        android:textColor="@color/yazi"
        android:textSize="20sp"
        app:layout_constraintEnd_toStartOf="@+id/textViewKirsofGerilimIR1"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewKirsofGerilimI" />

    <TextView
        android:id="@+id/textViewKirsofGerilimVR2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="VR2"
        android:textColor="@color/yazi"
        android:textSize="20sp"
        app:layout_constraintEnd_toStartOf="@+id/textViewKirsofGerilimIR2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewKirsofGerilimVR1" />

    <TextView
        android:id="@+id/textViewKirsofGerilimVR3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="VR3"
        android:textColor="@color/yazi"
        android:textSize="20sp"
        app:layout_constraintEnd_toStartOf="@+id/textViewKirsofGerilimIR3"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewKirsofGerilimVR2" />

    <Button
        android:id="@+id/buttonKirsofGerilimHesapla"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_marginTop="35dp"
        android:backgroundTint="@color/buton"
        android:text="@string/hesapla"
        android:textColor="@color/butonyazi"
        app:cornerRadius="15dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextKirsofGerilim" />

    <TextView
        android:id="@+id/textViewKirsofGerilimIRes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:gravity="center"
        android:text="Reş"
        android:textColor="@color/yazi"
        android:textSize="20sp"
        app:layout_constraintEnd_toStartOf="@+id/textViewKirsofGerilimI"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonKirsofGerilimHesapla" />

    <TextView
        android:id="@+id/textViewKirsofGerilimIR1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:gravity="center"
        android:text="IR1"
        android:textColor="@color/yazi"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/textViewKirsofGerilimVR1"
        app:layout_constraintTop_toBottomOf="@+id/textViewKirsofGerilimIRes" />

    <TextView
        android:id="@+id/textViewKirsofGerilimIR2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:gravity="center"
        android:text="IR2"
        android:textColor="@color/yazi"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/textViewKirsofGerilimVR2"
        app:layout_constraintTop_toBottomOf="@+id/textViewKirsofGerilimIR1" />

    <TextView
        android:id="@+id/textViewKirsofGerilimIR3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:gravity="center"
        android:text="IR3"
        android:textColor="@color/yazi"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/textViewKirsofGerilimVR3"
        app:layout_constraintTop_toBottomOf="@+id/textViewKirsofGerilimIR2" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="60dp"
        android:layout_height="20dp"
        android:layout_marginBottom="10dp"
        android:src="@drawable/voltageicon"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>