<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background"
    tools:context=".SeriRLC">

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:text="@string/serirlc"
        android:textColor="@color/renkli"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editTextSeriRLCR"
        android:layout_width="220dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:ems="10"
        android:hint="Direnç değerini giriniz (Ω)"
        android:inputType="number|numberDecimal"
        android:textAlignment="center"
        android:textColorHint="@color/renkli"
        android:textSize="14sp"
        app:layout_constraintEnd_toStartOf="@+id/buttonSeriRLCHesapla"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spinnerBobinBirim" />

    <EditText
        android:id="@+id/editTextSeriRLCL"
        android:layout_width="220dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:ems="10"
        android:hint="Bobin değerini giriniz"
        android:inputType="number|numberDecimal"
        android:textAlignment="center"
        android:textColorHint="@color/renkli"
        android:textSize="14sp"
        app:layout_constraintEnd_toStartOf="@+id/buttonSeriRLCHesapla"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextSeriRLCR" />

    <EditText
        android:id="@+id/editTextSeriRLCC"
        android:layout_width="220dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:ems="10"
        android:hint="Kondansatör değerini giriniz"
        android:inputType="number|numberDecimal"
        android:textAlignment="center"
        android:textColorHint="@color/renkli"
        android:textSize="14sp"
        app:layout_constraintEnd_toStartOf="@+id/buttonSeriRLCHesapla"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextSeriRLCL" />

    <EditText
        android:id="@+id/editTextSeriRLCV"
        android:layout_width="220dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:ems="10"
        android:hint="Gerilim değerini giriniz (V)"
        android:inputType="number|numberDecimal"
        android:textAlignment="center"
        android:textColorHint="@color/renkli"
        android:textSize="14sp"
        app:layout_constraintEnd_toStartOf="@+id/buttonSeriRLCHesapla"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextSeriRLCC" />

    <EditText
        android:id="@+id/editTextSeriRLCf"
        android:layout_width="220dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:ems="10"
        android:hint="Frekans değerini giriniz (Hz)"
        android:inputType="number|numberDecimal"
        android:textAlignment="center"
        android:textColorHint="@color/renkli"
        android:textSize="14sp"
        app:layout_constraintEnd_toStartOf="@+id/buttonSeriRLCHesapla"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextSeriRLCV" />

    <Button
        android:id="@+id/buttonSeriRLCHesapla"
        android:layout_width="90dp"
        android:layout_height="100dp"
        android:layout_marginEnd="20dp"
        android:backgroundTint="@color/mavisari"
        android:foreground="@drawable/hesapla"
        android:foregroundGravity="center"
        android:foregroundTint="@color/background"
        android:textAlignment="center"
        app:cornerRadius="100dp"
        app:layout_constraintBottom_toBottomOf="@+id/editTextSeriRLCf"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/editTextSeriRLCR" />

    <TextView
        android:id="@+id/textViewSeriRLCXL"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:textAlignment="center"
        android:textColor="@color/yazi"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextSeriRLCf" />

    <TextView
        android:id="@+id/textViewSeriRLCXC"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="60dp"
        android:textAlignment="center"
        android:textColor="@color/yazi"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextSeriRLCf" />

    <TextView
        android:id="@+id/textViewSeriRLCZ"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="90dp"
        android:textAlignment="center"
        android:textColor="@color/yazi"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextSeriRLCf" />

    <TextView
        android:id="@+id/textViewSeriRLCVR"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="150dp"
        android:textAlignment="center"
        android:textColor="@color/yazi"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextSeriRLCf" />

    <TextView
        android:id="@+id/textViewSeriRLCVL"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="180dp"
        android:textAlignment="center"
        android:textColor="@color/yazi"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextSeriRLCf" />

    <TextView
        android:id="@+id/textViewSeriRLCVC"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="210dp"
        android:textAlignment="center"
        android:textColor="@color/yazi"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextSeriRLCf" />

    <TextView
        android:id="@+id/textViewSeriRLCi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="120dp"
        android:textAlignment="center"
        android:textColor="@color/yazi"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextSeriRLCf" />

    <Spinner
        android:id="@+id/spinnerBobinBirim"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        app:layout_constraintEnd_toStartOf="@+id/spinnerKondansatorBirim"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView19" />

    <Spinner
        android:id="@+id/spinnerKondansatorBirim"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/spinnerBobinBirim"
        app:layout_constraintTop_toBottomOf="@+id/textView19" />

    <com.google.android.gms.ads.AdView
        android:id="@+id/adView"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        app:adSize="BANNER"
        app:adUnitId="ca-app-pub-1879393212966846/1948543313"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>